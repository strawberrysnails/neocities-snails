<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <link rel="icon" href="{{ site.favicon }}" type="image/png"> <!-- Credit: https://pixelsafari.neocities.org/favicon/2 -->
        <title>{{ site.title }}{% if title %} | {{ title }}{% endif %}</title>
            <!-- CSS/JS + CACHEBUSTING -->
            <link rel="stylesheet" href="/css/dg3.css?nocache={{ nocache }}">
            <link rel="stylesheet" href="/css/fonts.css">
            <link rel="stylesheet" href="/css/bootstrap-icons.css">

  </head>
  <body>
<!--MUSIC PLAYER -->
<div class="simple-player">
  <img class="cover" src="https://files.catbox.moe/kc2d48.jpg" alt="Cover">
  
  <div class="info">
    <div class="song-title"><span>Song Title Goes Here</span></div>
    <div class="controls">
<div class="controls">
  <button class="prev-track" onclick="prevTrack()">⏮</button>
  <button class="playpause-track" onclick="playpauseTrack()"><i class="bi bi-play-fill"></i></button>
  <button class="next-track" onclick="nextTrack()">⏭</button>
</div>

    </div>
    <div class="progress">
      <span class="current-time">0:00</span>
      <input type="range" min="0" max="100" value="0" class="seek-slider" onchange="seekTo()">
      <span class="total-duration">0:00</span>
    </div>
  </div>

  <audio id="music" autoplay src=""></audio>
</div>

<!-- END MUSIC PLAYER -->
    <img class ="logo" src="/images/dg3/logo.png">
    <div class="container">
                        {{ content | safe }}
    </div>
    <button id="myBtn" title="Go to top"><i class="bi bi-arrow-up-square-fill"></i></button>
        <script src="/javascript/main.js"></script>
        <script src="/javascript/musicplayer.js"></script>
</body>
</html>